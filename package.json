    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
