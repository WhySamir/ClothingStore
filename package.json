{
  "name": "clothingstore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint --no-error-on-warn",
    "test": "env-cmd -f .env.test cross-env NODE_ENV=test jest --runInBand",
    "test:docker": "docker-compose -f docker-compose.test.yml up -d && env-cmd -f .env.test npx prisma generate && env-cmd -f .env.test npx prisma db push --force-reset && env-cmd -f .env.test cross-env NODE_ENV=test jest --runInBand"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed.ts"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.2.7",
    "@hookform/resolvers": "^5.2.1",
